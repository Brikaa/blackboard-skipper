{
    "name": "Faster BlackBoard",
    "description": "Skip the intermediate page that BlackBoard shows when opening a PDF",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["*://*.blackboard.com/webapps/blackboard/content/*"],
            "js": ["index.js"],
            "run_at": "document_idle",
            "all_frames": true
        }
    ],
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "64": "images/64.png",
        "128": "images/128.png"
    },
    "permissions": []
}
